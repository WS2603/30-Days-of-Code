for n in range(int(input())):
    s = input()
    # s[::2] will retrieve all array items starting from index 0, incrementing the index two by two and accessing all even indexes (0,2,4,6, and so on).
    # s[1::2] will retrieve all array items, from index 1, incrementing the index two by two and accessing all odd indexes (1,3,5,7,9, and so on).
    print(s[::2], s[1::2])
